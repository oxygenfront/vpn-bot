{
  "$schema": "https://biomejs.dev/schemas/1.9.4/schema.json",
  "linter": {
    "enabled": true,
    "rules": {
      "a11y": {
        "noAccessKey": "warn",
        "noAriaHiddenOnFocusable": "warn",
        "noAriaUnsupportedElements": "warn",
        "noAutofocus": "warn",
        "noBlankTarget": "error",
        "noDistractingElements": "warn",
        "noHeaderScope": "warn",
        "noInteractiveElementToNoninteractiveRole": "warn",
        "noNoninteractiveElementToInteractiveRole": "warn",
        "noNoninteractiveTabindex": "warn",
        "noPositiveTabindex": "warn",
        "noRedundantAlt": "warn",
        "noRedundantRoles": "warn",
        "noSvgWithoutTitle": "warn",
        "useAltText": "error",
        "useAnchorContent": "warn",
        "useAriaActivedescendantWithTabindex": "warn",
        "useAriaPropsForRole": "warn",
        "useButtonType": "warn",
        "useHeadingContent": "warn",
        "useHtmlLang": "error",
        "useIframeTitle": "warn",
        "useKeyWithClickEvents": "warn",
        "useKeyWithMouseEvents": "warn",
        "useMediaCaption": "warn",
        "useValidAnchor": "warn",
        "useValidAriaProps": "warn",
        "useValidAriaRole": "warn",
        "useValidAriaValues": "warn",
        "useValidLang": "error"
      },
      "complexity": {
        "noBannedTypes": "warn",
        "noEmptyTypeParameters": "error",
        "noExcessiveCognitiveComplexity": "warn",
        "noExcessiveNestedTestSuites": "error",
        "noExtraBooleanCast": "warn",
        "noForEach": "warn",
        "noMultipleSpacesInRegularExpressionLiterals": "warn",
        "noStaticOnlyClass": "error",
        "noThisInStatic": "error",
        "noUselessCatch": "warn",
        "noUselessConstructor": "error",
        "noUselessEmptyExport": "error",
        "noUselessFragments": "error",
        "noUselessLabel": "warn",
        "noUselessLoneBlockStatements": "error",
        "noUselessRename": "error",
        "noUselessSwitchCase": "warn",
        "noUselessTernary": "warn",
        "noUselessThisAlias": "error",
        "noUselessTypeConstraint": "error",
        "noVoid": "error",
        "noWith": "error",
        "useArrowFunction": "warn",
        "useFlatMap": "warn",
        "useLiteralKeys": "warn",
        "useOptionalChain": "warn",
        "useRegexLiterals": "warn",
        "useSimpleNumberKeys": "warn",
        "useSimplifiedLogicExpression": "warn"
      },
      "correctness": {
        "noChildrenProp": "error",
        "noConstAssign": "error",
        "noConstantCondition": "error",
        "noConstructorReturn": "error",
        "noEmptyCharacterClassInRegex": "error",
        "noEmptyPattern": "error",
        "noGlobalObjectCalls": "error",
        "noInnerDeclarations": "warn",
        "noInvalidConstructorSuper": "error",
        "noInvalidNewBuiltin": "error",
        "noInvalidUseBeforeDeclaration": "error",
        "noNewSymbol": "error",
        "noNonoctalDecimalEscape": "warn",
        "noPrecisionLoss": "error",
        "noRenderReturnValue": "warn",
        "noSelfAssign": "error",
        "noSetterReturn": "error",
        "noStringCaseMismatch": "error",
        "noSwitchDeclarations": "warn",
        "noUndeclaredVariables": "off",
        "noUnnecessaryContinue": "warn",
        "noUnreachable": "error",
        "noUnreachableSuper": "error",
        "noUnsafeFinally": "warn",
        "noUnsafeOptionalChaining": "warn",
        "noUnusedImports": "error",
        "noUnusedLabels": "warn",
        "noUnusedPrivateClassMembers": "warn",
        "noUnusedVariables": "warn",
        "noVoidElementsWithChildren": "error",
        "noVoidTypeReturn": "warn",
        "useExhaustiveDependencies": "off",
        "useHookAtTopLevel": "error",
        "useIsNan": "error",
        "useJsxKeyInIterable": "error",
        "useValidForDirection": "warn",
        "useYield": "warn"
      },
      "nursery": {
        "noRestrictedImports": "warn",
        "useImportRestrictions": "off",
        "useSortedClasses": "warn"
      },
      "performance": {
        "noAccumulatingSpread": "warn",
        "noBarrelFile": "off",
        "noDelete": "warn",
        "noReExportAll": "error"
      },
      "security": {
        "noDangerouslySetInnerHtml": "error",
        "noDangerouslySetInnerHtmlWithChildren": "error",
        "noGlobalEval": "error"
      },
      "style": {
        "noArguments": "warn",
        "noCommaOperator": "warn",
        "noDefaultExport": "warn",
        "noImplicitBoolean": "off",
        "noInferrableTypes": "warn",
        "noNamespace": "warn",
        "noNamespaceImport": "error",
        "noNegationElse": "warn",
        "noNonNullAssertion": "error",
        "noParameterAssign": "warn",
        "noParameterProperties": "off",
        "noRestrictedGlobals": "warn",
        "noShoutyConstants": "warn",
        "noUnusedTemplateLiteral": "warn",
        "noUselessElse": "error",
        "noVar": "error",
        "useAsConstAssertion": "warn",
        "useBlockStatements": "warn",
        "useCollapsedElseIf": "warn",
        "useConsistentArrayType": {
          "level": "warn",
          "options": {
            "syntax": "shorthand"
          }
        },
        "useConst": "error",
        "useDefaultParameterLast": "warn",
        "useEnumInitializers": "error",
        "useExponentiationOperator": "warn",
        "useExportType": "warn",
        "useFilenamingConvention": {
          "level": "warn",
          "options": {
            "filenameCases": [
              "kebab-case",
              "camelCase"
            ],
            "strictCase": false
          }
        },
        "useForOf": "warn",
        "useFragmentSyntax": "warn",
        "useImportType": "off",
        "useLiteralEnumMembers": "error",
        "useNamingConvention": {
          "level": "off",
          "options": {
            "enumMemberCase": "CONSTANT_CASE",
            "requireAscii": true,
            "strictCase": false
          }
        },
        "useNodeAssertStrict": "warn",
        "useNodejsImportProtocol": "warn",
        "useNumberNamespace": "error",
        "useNumericLiterals": "warn",
        "useSelfClosingElements": "warn",
        "useShorthandArrayType": "warn",
        "useShorthandAssign": "warn",
        "useShorthandFunctionType": "warn",
        "useSingleCaseStatement": "warn",
        "useSingleVarDeclarator": "error",
        "useTemplate": "error",
        "useWhile": "warn"
      },
      "suspicious": {
        "noApproximativeNumericConstant": "warn",
        "noArrayIndexKey": "error",
        "noAssignInExpressions": "warn",
        "noAsyncPromiseExecutor": "warn",
        "noCatchAssign": "warn",
        "noClassAssign": "warn",
        "noCommentText": "warn",
        "noCompareNegZero": "warn",
        "noConfusingLabels": "error",
        "noConfusingVoidType": "error",
        "noConsoleLog": "warn",
        "noConstEnum": "warn",
        "noControlCharactersInRegex": "warn",
        "noDebugger": "warn",
        "noDoubleEquals": "error",
        "noDuplicateCase": "warn",
        "noDuplicateClassMembers": "error",
        "noDuplicateJsxProps": "error",
        "noDuplicateObjectKeys": "error",
        "noDuplicateParameters": "error",
        "noDuplicateTestHooks": "error",
        "noEmptyBlockStatements": "warn",
        "noEmptyInterface": "warn",
        "noExplicitAny": "warn",
        "noExportsInTest": "error",
        "noExtraNonNullAssertion": "error",
        "noFallthroughSwitchClause": "warn",
        "noFocusedTests": "warn",
        "noFunctionAssign": "warn",
        "noGlobalAssign": "error",
        "noGlobalIsFinite": "warn",
        "noGlobalIsNan": "warn",
        "noImplicitAnyLet": "warn",
        "noImportAssign": "error",
        "noLabelVar": "error",
        "noMisleadingCharacterClass": "warn",
        "noMisleadingInstantiator": "warn",
        "noMisrefactoredShorthandAssign": "warn",
        "noPrototypeBuiltins": "error",
        "noRedeclare": "error",
        "noRedundantUseStrict": "warn",
        "noSelfCompare": "warn",
        "noShadowRestrictedNames": "warn",
        "noSkippedTests": "warn",
        "noSparseArray": "error",
        "noThenProperty": "error",
        "noUnsafeDeclarationMerging": "warn",
        "noUnsafeNegation": "warn",
        "useAwait": "warn",
        "useDefaultSwitchClauseLast": "warn",
        "useGetterReturn": "error",
        "useIsArray": "warn",
        "useNamespaceKeyword": "warn",
        "useValidTypeof": "error"
      }
    }
  }
}